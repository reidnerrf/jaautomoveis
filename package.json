{
  "name": "ja-automoveis-fullstack",
  "version": "1.0.0",
  "description": "Full-stack application for JA AutomÃ³veis.",
  "main": "dist/server.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "start:nodb": "cross-env NODE_ENV=production SKIP_DB=true node dist/server.js",
    "start:full": "bash -lc \"docker compose up -d --build mongo app nginx || docker-compose up -d --build mongo app nginx\"",
    "stop:full": "bash -lc \"docker compose down || docker-compose down\"",
    "db:up": "bash -lc \"docker compose up -d mongo || docker-compose up -d mongo\"",
    "wait:db": "wait-on -t 120000 tcp:127.0.0.1:27017",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "cross-env NODE_ENV=development nodemon --watch server.ts --watch backend --ext ts,tsx --exec ts-node --project tsconfig.server.json --transpile-only server.ts",
    "dev:server:nodb": "cross-env NODE_ENV=development SKIP_DB=true nodemon --watch server.ts --watch backend --ext ts,tsx --exec ts-node --project tsconfig.server.json --transpile-only server.ts",
    "seed": "ts-node seeder.ts",
    "analyze": "npm run build:client && npx vite-bundle-analyzer dist/assets",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch --config jest.config.cjs",
    "test:unit": "jest --config jest.config.cjs --testPathPattern=tests/unit",
    "test:api": "jest --config jest.config.cjs --testPathPattern=tests/api",
    "test:e2e": "playwright test",
    "test:performance": "ts-node tests/performance/lighthouse.test.ts",
    "test:all": "npm run lint && npm run type-check && npm run test && npm run test:e2e",
    "clean": "rm -rf dist node_modules/.cache",
    "preview": "vite preview",
    "generate-static": "ts-node utils/ssr.ts",
    "docker:build": "docker build -t ja-automoveis .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "performance:audit": "npm run test:performance && npm run test:e2e",
    "ci": "npm run lint && npm run type-check && npm run test && npm run test:e2e",
    "format": "prettier --write ."
  },
